select * from safe_user

select
         caup.leftstr(:person_data, '-'),caup.rightstr(:person_data, '-') into  v_person_code,v_person_type 
      from dual;
      
      select 
               user_role_name as "user_role_name"
            from 
               safe_user_roles
            where 
               user_role_name like :search_text